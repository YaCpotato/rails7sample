<!DOCTYPE html>
<html>
  <head>
    <title>Myapp</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= javascript_importmap_tags %>
  </head>

  <body>
    <%= link_to "/auth/google_oauth2", "/auth/google_oauth2", method: :post %>
    <%= link_to "/auth/google", "/auth/google", method: :post %>
    <%= link_to "/auth/google/callback", "/auth/google/callback", method: :post %>
    <%= yield %>
  </body>
</html>
